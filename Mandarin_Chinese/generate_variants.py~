tones = ['˥','˧˥','˨˩˦','˥˩','']
accents = [
    {'a':'ā', 'e':'ē', 'i':'ī', 'o':'ō', 'u':'ū', 'ü':'ǖ'},
    {'a':'á', 'e':'é', 'i':'í', 'o':'ó', 'u':'ú', 'ü':'ǘ'},
    {'a':'ǎ', 'e':'ě', 'i':'ǐ', 'o':'ǒ', 'u':'ǔ', 'ü':'ǚ'},
    {'a':'à', 'e':'è', 'i':'ì', 'o':'ò', 'u':'ù', 'ü':'ǜ'}
]

def genseq(D,n,gtype):
    for pair in D:
        x = pair[0]
        y = pair[1]
        if gtype=='accent' and n < 5:
            repl = accents[n-1][x[0][-1]]
            g = x[0][:-1]+repl+x[1]
        elif gtype=='number':
            g = '%s%s%d'%(x[0],x[1],n)
        else:
            g = x[0]+x[1]
        print('%s\t%s%s %s'%(g,y[0],tones[n-1],y[1]))

if __name__=="__main__":
    genseq(initials,5,'accent')
    for n in range(1,6):
        genseq(finals,n,'accent')
        genseq(finals,n,'number')

    
